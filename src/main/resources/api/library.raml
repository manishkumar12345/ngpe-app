  AddPatientDataType: !include DataTypes/AddPatient.raml
  UpdatePatientDataType: !include DataTypes/UpdatePatient.raml
  AddAppointmentDataType: !include DataTypes/AppointmentDataType.raml
  UpdateAppointmentDataType: !include DataTypes/UpdateAppointmentDataType.raml
  ApprovedAppoinmentDataType: !include DataTypes/ApprovedAppoinmentDataType.raml
  CanceledAppoinmentDataType: !include DataTypes/CanceledAppoinmentDataType.raml

